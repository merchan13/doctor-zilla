    <div class="col-md-3">
      <input id="cc_normal" name="physical[cc]" type="radio" value="0" checked="true" onclick="javascript:normalAbnormalCheck('cc');" />
      <span>Normal</span>
    </div>
    <div class="col-md-3">
      <input id="cc_abnormal" name="physical[cc]" type="radio" value="1" onclick="javascript:normalAbnormalCheck('cc');" />
      <span>Anormal</span>
    </div>
  </div>
  <!-- descripcion + esquema -->
  <div id="if_cc_abnormal" class="col-md-12" style="display:none">
    <div class="col-md-6">
      <%= text_area(:physical_description, :cc, rows: '3', class: 'form-control') %>
